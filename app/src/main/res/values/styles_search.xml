<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextTrackView">
        <item name="android:textColor">@color/color_track_view_search_screen</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textSize">11sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="SearchScreenContainers">
        <item name="android:textColor">@color/color_container_info</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">19sp</item>
    </style>

    <style name="SearchScreenButtons">
        <item name="android:textColor">@color/background_search_screen</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:backgroundTint">@color/color_container_info</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">50dp</item>
    </style>

</resources>